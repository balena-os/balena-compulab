From 7540aaacb2bcdaf3e19fe26424ad940e65525c3a Mon Sep 17 00:00:00 2001
From: Sebastian Panceac <sebastian@balena.io>
Date: Mon, 16 Sep 2019 15:54:36 +0200
Subject: [PATCH] niste loguri

---
 drivers/gpu/drm/drm_fb_cma_helper.c | 2 ++
 drivers/gpu/drm/drm_fb_helper.c     | 4 +++-
 2 files changed, 5 insertions(+), 1 deletion(-)

diff --git a/drivers/gpu/drm/drm_fb_cma_helper.c b/drivers/gpu/drm/drm_fb_cma_helper.c
index f2ee88363015..5db05b5a9d68 100644
--- a/drivers/gpu/drm/drm_fb_cma_helper.c
+++ b/drivers/gpu/drm/drm_fb_cma_helper.c
@@ -402,6 +402,8 @@ struct drm_fbdev_cma *drm_fbdev_cma_init_with_funcs(struct drm_device *dev,
 	struct drm_fb_helper *helper;
 	int ret;
 
+	printk(KERN_ERR "SEBIX %s\n", __FUNCTION__);
+
 	fbdev_cma = kzalloc(sizeof(*fbdev_cma), GFP_KERNEL);
 	if (!fbdev_cma) {
 		dev_err(dev->dev, "Failed to allocate drm fbdev.\n");
diff --git a/drivers/gpu/drm/drm_fb_helper.c b/drivers/gpu/drm/drm_fb_helper.c
index 8549d5e128db..0e42ebe18c5e 100644
--- a/drivers/gpu/drm/drm_fb_helper.c
+++ b/drivers/gpu/drm/drm_fb_helper.c
@@ -1854,6 +1854,7 @@ static int drm_fb_helper_single_fb_probe(struct drm_fb_helper *fb_helper,
 			sizes.fb_height = min_t(u32, desired_mode->vdisplay + y, sizes.fb_height);
 	}
 
+	printk(KERN_ERR "SEB crtc %d, wid %d, heig %d\n", crtc_count, sizes.fb_width, sizes.fb_height);
 	if (crtc_count == 0 || sizes.fb_width == -1 || sizes.fb_height == -1) {
 		DRM_INFO("Cannot find any crtc or sizes\n");
 
@@ -2347,6 +2348,7 @@ static void drm_setup_crtcs(struct drm_fb_helper *fb_helper,
 	bool *enabled;
 	int i;
 
+	printk(KERN_ERR "SEBIX %s\n", __FUNCTION__);
 	DRM_DEBUG_KMS("\n");
 	/* prevent concurrent modification of connector_count by hotplug */
 	lockdep_assert_held(&fb_helper->lock);
@@ -2560,7 +2562,7 @@ int drm_fb_helper_initial_config(struct drm_fb_helper *fb_helper, int bpp_sel)
 
 	if (!drm_fbdev_emulation)
 		return 0;
-
+	printk(KERN_ERR "SEBIX %s", __FUNCTION__);
 	mutex_lock(&fb_helper->lock);
 	ret = __drm_fb_helper_initial_config_and_unlock(fb_helper, bpp_sel);
 
-- 
